{
  "extends": "../tsconfig-config/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM"],
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./dist-web",
    "paths": {
      "leather-styles/*": ["leather-styles/*"]
    }
  },
  "include": [
    "**/*.web.{ts,tsx}",
    "**/*.web.{ts,tsx}",
    "web.ts",
    "**/*.shared.tsx",
    "global.d.ts",
    "leather-styles"
  ],
  "exclude": [
    "node_modules",
    "babel.config.cjs",
    "metro.config.cjs",
    "tsup.config.*.ts",
    "**/*.js",
    "./dist-*"
  ]
}
