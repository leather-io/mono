{
  "extends": "../tsconfig-config/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["es2017", "DOM"],
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./dist-web",
    "paths": {
      "leather-styles/*": ["leather-styles/*"]
    }
  },
  "include": ["**/*.web.ts", "**/*.web.tsx", "web.ts"],
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "**/*.js", "./dist-*"]
}
