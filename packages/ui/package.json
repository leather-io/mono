{
  "name": "@leather-wallet/ui",
  "version": "1.3.1",
  "type": "module",
  "scripts": {
    "assets:native": "mkdir -p dist-native/src && cp -r src/assets-native ./dist-native/src/",
    "assets:web": "mkdir -p dist-web/src && cp -r src/assets-web ./dist-web/src",
    "build": "pnpm panda && pnpm build:native && pnpm build:web && pnpm build:all",
    "build-storybook:web": "storybook build -c ./src/.storybook-web",
    "build:all": "pnpm assets:web && pnpm assets:native && tsc --build ./tsconfig.json",
    "build:native": "pnpm assets:native && tsc -p ./tsconfig.native.json",
    "build:watch": "concurrently  \"pnpm panda:watch\"  \"pnpm build:native --watch --preserveWatchOutput\"  \"pnpm build:web --watch --preserveWatchOutput\"  \"pnpm build:all --watch --preserveWatchOutput\" ",
    "build:web": "pnpm assets:web && tsc -p ./tsconfig.web.json",
    "format": "prettier . --write \"src/**/*.{ts,tsx}\" --ignore-path ../../.prettierignore",
    "format:check": "prettier . --check \"src/**/*.{ts,tsx}\" --ignore-path ../../.prettierignore",
    "lint": "eslint . --ignore-path ../../.eslintignore",
    "lint:fix": "eslint . --fix --ignore-path ../../.eslintignore",
    "panda": "panda codegen -c ./panda.config.ts",
    "panda:watch": "panda codegen -c ./panda.config.ts --watch",
    "storybook-docs:web": "storybook dev --docs -c ./src/.storybook-web",
    "storybook:android": "expo start --android",
    "storybook:ios": "expo start --ios",
    "storybook:web": "storybook dev -p 6006 -c ./src/.storybook-web",
    "typecheck": "tsc --noEmit --project ./tsconfig.json"
  },
  "exports": {
    ".": "./dist-web/web.js",
    "./native": "./dist-native/native.js"
  },
  "dependencies": {
    "@expo/vector-icons": "14.0.0",
    "@leather-wallet/tokens": "workspace:*",
    "@radix-ui/react-avatar": "1.0.4",
    "@react-native/assets-registry": "0.73.1",
    "@react-native/metro-config": "0.73.5",
    "@rnx-kit/metro-config": "1.3.14",
    "@rnx-kit/metro-resolver-symlinks": "0.1.35",
    "@shopify/restyle": "2.4.2",
    "expo": "51.0.8",
    "expo-asset": "10.0.6",
    "expo-constants": "16.0.1",
    "expo-font": "12.0.5",
    "expo-splash-screen": "0.27.4",
    "metro-cache": "0.80.5",
    "metro-config": "0.80.5",
    "metro-resolver": "0.80.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1"
  },
  "devDependencies": {
    "@babel/core": "7.24.6",
    "@babel/runtime": "7.23.2",
    "@leather-wallet/eslint-config": "workspace:*",
    "@leather-wallet/panda-preset": "workspace:*",
    "@pandacss/dev": "0.26.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@storybook/addon-actions": "7.6.17",
    "@storybook/addon-docs": "7.6.15",
    "@storybook/addon-essentials": "7.6.15",
    "@storybook/addon-interactions": "7.6.15",
    "@storybook/addon-links": "7.6.15",
    "@storybook/addon-onboarding": "1.0.10",
    "@storybook/addon-ondevice-actions": "7.6.15",
    "@storybook/addon-ondevice-controls": "7.6.15",
    "@storybook/addon-styling-webpack": "0.0.5",
    "@storybook/blocks": "7.6.15",
    "@storybook/manager-api": "7.6.17",
    "@storybook/react": "7.6.15",
    "@storybook/react-native": "7.6.15",
    "@storybook/react-webpack5": "7.6.15",
    "@storybook/test": "7.6.15",
    "@storybook/theming": "7.6.17",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "babel-preset-expo": "11.0.6",
    "concurrently": "8.2.2",
    "css-loader": "6.10.0",
    "eslint": "8.53.0",
    "eslint-config-universe": "12.0.0",
    "postcss-loader": "8.1.1",
    "postcss-preset-env": "9.4.0",
    "storybook": "7.6.15",
    "style-loader": "3.3.4",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "tslib": "2.6.2",
    "typescript": "5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
