{
  "name": "@leather.io/stacks",
  "author": "leather.io",
  "description": "Stacks package for Leather",
  "version": "1.5.10",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch --onSuccess 'tsup --dts-only'",
    "prepublish": "pnpm build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "typecheck": "tsc --noEmit",
    "types": "tsc --declaration --emitDeclarationOnly"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "dependencies": {
    "@leather.io/constants": "workspace:*",
    "@leather.io/crypto": "workspace:*",
    "@leather.io/models": "workspace:*",
    "@leather.io/utils": "workspace:*",
    "@noble/hashes": "1.5.0",
    "@scure/bip32": "1.5.0",
    "@stacks/encryption": "6.16.1",
    "@stacks/network": "6.13.0",
    "@stacks/stacks-blockchain-api-types": "7.8.2",
    "@stacks/transactions": "6.17.0",
    "bignumber.js": "9.1.2"
  },
  "devDependencies": {
    "tsup": "8.1.0",
    "vitest": "2.1.5"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
