  "dependencies": {
    "@ledgerhq/hw-transport-webusb": "^6.27.1",
    "ledger-bitcoin": "^0.7.0",
    "@zondax/ledger-stacks": "^0.19.0",
    "@leather.io/bitcoin": "workspace:*",
    "@leather.io/models": "workspace:*",
    "@leather.io/utils": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
