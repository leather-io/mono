{
  "compilerOptions": {
    "allowJs": false,
    "checkJs": false,
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "Bundler",
    "lib": ["dom", "dom.iterable", "es2022"],
    "sourceMap": true,
    "removeComments": false,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "incremental": false,
    "composite": false,
    "declaration": true,
    "declarationMap": true,
    "isolatedModules": true,
    "types": ["jest"]
  }
}
