{
  "name": "leather-mono",
  "author": "Leather Wallet",
  "description": "Leather wallet shared code",
  "version": "2.7.2",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "build:watch": "turbo run build:watch",
    "check:all": "turbo run check:all",
    "clean": "git clean -dfX",
    "dev": "turbo run build:watch",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "install:clean": "rm -rf \"**/node_modules\" && pnpm -r clean",
    "install:fresh": "pnpm install:clean && pnpm i",
    "install:nuke": "rm -rf pnpm-lock.yaml && pnpm install:fresh",
    "lint": "eslint --cache --max-warnings 0",
    "lint:deps": "pnpm dependency-cruiser .",
    "lint:filenames": "pnpm ls-lint",
    "lint:fix": "pnpm lint --fix",
    "prepare": "husky",
    "syncpack:fix": "syncpack fix-mismatches",
    "syncpack:format": "syncpack format",
    "syncpack:lint": "syncpack lint",
    "test": "turbo run test",
    "test:coverage": "pnpm -r test:coverage",
    "test:unit": "pnpm -r test:unit",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@babel/runtime": "7.26.0",
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "19.6.0",
    "@leather.io/eslint-config": "workspace:*",
    "@leather.io/prettier-config": "workspace:*",
    "@leather.io/tsconfig-config": "workspace:*",
    "@ls-lint/ls-lint": "2.2.3",
    "@tanstack/eslint-plugin-query": "5.62.1",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@vitest/coverage-v8": "2.1.5",
    "chalk": "5.3.0",
    "dependency-cruiser": "16.6.0",
    "dotenv": "16.4.5",
    "eslint": "9.16.0",
    "eslint-plugin-lingui": "0.4.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "prettier": "3.5.1",
    "syncpack": "13.0.0",
    "turbo": "2.4.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.18.0",
    "vitest": "2.1.9"
  },
  "engines": {
    "node": ">=20.11.0",
    "pnpm": ">=10.4.0",
    "npm": "please-use-pnpm",
    "yarn": "please-use-pnpm"
  },
  "keywords": [],
  "packageManager": "pnpm@10.4.1",
  "pnpm": {
    "overrides": {
      "cross-spawn": "7.0.5"
    },
    "packageExtensions": {
      "@expo/cli@*": {
        "dependencies": {
          "metro": "*",
          "metro-core": "*",
          "metro-resolver": "*",
          "glob": "*",
          "@expo/image-utils": "*",
          "lodash": "*",
          "react-native": "*",
          "react-native-web": "*",
          "metro-config": "*",
          "@react-native-community/cli-server-api": "*",
          "metro-runtime": "*",
          "@expo/server": "*"
        }
      },
      "babel-preset-expo@*": {
        "dependencies": {
          "expo": "*",
          "debug": "*",
          "resolve-from": "*",
          "expo-router": "*",
          "react-native-reanimated": "*"
        }
      },
      "@react-native/dev-middleware@*": {
        "dependencies": {
          "ws": "*"
        }
      },
      "@expo/metro-config@*": {
        "dependencies": {
          "metro-config": "*",
          "metro-cache": "*",
          "metro-cache-key": "*",
          "expo-asset": "*",
          "metro": "*",
          "metro-transform-plugins": "*",
          "metro-source-map": "*",
          "metro-transform-worker": "*",
          "metro-minify-terser": "*",
          "@react-native/js-polyfills": "*"
        },
        "peerDependencies": {
          "expo-asset": "*"
        }
      },
      "@expo/metro-runtime@*": {
        "dependencies": {
          "react-native": "*",
          "url-parse": "*",
          "expo-constants": "*",
          "stacktrace-parser": "*"
        },
        "peerDependencies": {
          "react-native": "*"
        }
      },
      "expo-head@*": {
        "dependencies": {
          "url-parse": "*"
        }
      },
      "expo-router@*": {
        "dependencies": {
          "react-native": "*",
          "nanoid": "*",
          "url-parse": "*",
          "resolve-from": "*",
          "debug": "*",
          "@react-navigation/native-stack": "*",
          "@react-navigation/core": "*",
          "@expo/metro-runtime": "*",
          "expo-constants": "*",
          "escape-string-regexp": "*"
        }
      },
      "expo-linking@*": {
        "dependencies": {
          "expo-modules-core": "*"
        }
      },
      "expo-web-browser@*": {
        "dependencies": {
          "expo-modules-core": "*"
        }
      },
      "expo-font@*": {
        "dependencies": {
          "expo-modules-core": "*",
          "expo-constants": "*",
          "@expo/vector-icons": "*"
        }
      },
      "expo-splash-screen@*": {
        "dependencies": {
          "expo-modules-core": "*"
        }
      },
      "expo-constants@*": {
        "dependencies": {
          "expo-modules-core": "*"
        }
      },
      "expo@*": {
        "dependencies": {
          "@expo/metro-config": "*"
        }
      },
      "object-inspect@*": {
        "dependencies": {
          "util": "*"
        }
      },
      "@expo/vector-icons": {
        "dependencies": {
          "prop-types": "*"
        }
      },
      "react-native@*": {
        "peerDependencies": {
          "@react-native/assets-registry": "*"
        }
      },
      "expo-asset@*": {
        "dependencies": {
          "expo-modules-core": "*",
          "@expo/cli": "*"
        },
        "peerDependencies": {
          "@react-native/assets-registry": "*",
          "expo-font": "*"
        }
      },
      "expo-keep-awake@*": {
        "dependencies": {
          "expo-modules-core": "*"
        }
      },
      "expo-file-system": {
        "dependencies": {
          "expo-modules-core": "*"
        }
      }
    },
    "patchedDependencies": {
      "expo-secure-store@13.0.2": "patches/expo-secure-store@13.0.2.patch",
      "pofile@1.1.4": "patches/pofile@1.1.4.patch",
      "expo-blur@13.0.2": "patches/expo-blur@13.0.2.patch"
    }
  },
  "resolutions": {
    "axios": "1.8.2",
    "cookie": "0.7.1",
    "esbuild": "0.25.0",
    "micromatch": "4.0.8",
    "path-to-regexp": "0.1.12",
    "send": "0.19.0",
    "store2": "2.14.4",
    "undici": "6.21.1",
    "vite": "5.4.12"
  }
}
