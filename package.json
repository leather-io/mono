{
  "name": "leather-mono",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "install:clean": "rm -rf \"**/node_modules\" && pnpm -r clean",
    "install:fresh": "pnpm install:clean && pnpm i",
    "install:nuke": "rm -rf pnpm-lock.yaml && pnpm install:fresh",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:eslint:fix": "pnpm lint:eslint --fix",
    "lint:prettier": "prettier --check \"*.{ts,tsx,js,json}\"",
    "lint:prettier:fix": "prettier --write \"*.{ts,tsx,js,json}\" *.js",
    "lint:types": "tsc --noEmit",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@leather-wallet/eslint-config": "workspace:^",
    "@leather-wallet/prettier-config": "workspace:^",
    "@leather-wallet/tsconfig-config": "workspace:^",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "syncpack": "^11.2.1",
    "typescript": "^5.2.2",
    "lint-staged": "^15.0.2"
  },
  "packageManager": "pnpm@7.10.0"
}
