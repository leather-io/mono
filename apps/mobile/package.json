{
  "name": "@leather.io/mobile",
  "version": "2.54.1",
  "type": "module",
  "scripts": {
    "1password:env:dev": "op read \"op://leather-cli/env-content/dev\" > .env",
    "1password:env:prod": "op read \"op://leather-cli/env-content/prod\" > .env",
    "android": "expo run:android",
    "android:release": "pnpm run firebase:android && expo run:android --variant release",
    "build": "tsc --build",
    "build:watch": "tsc --build --watch --preserveWatchOutput",
    "check:cocoapods": "sh scripts/enforce-cocoapods-version.sh",
    "crowdin": "crowdin",
    "crowdin:sync": "crowdin push && crowdin pull",
    "crowdin:sync:sources": "crowdin push",
    "crowdin:sync:translations": "crowdin pull",
    "eas-build-post-install": "cd ../.. && pnpm install && pnpm build",
    "eas-build-pre-install": "corepack enable",
    "firebase": "pnpm run firebase:ios && pnpm run firebase:android",
    "firebase:android": "firebase login && firebase apps:sdkconfig ANDROID --project leather-b5081 > google-services.json",
    "firebase:ios": "firebase login && firebase apps:sdkconfig IOS --project leather-b5081 > GoogleService-Info.plist",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" --ignore-path ../../.prettierignore",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" --ignore-path ../../.prettierignore",
    "ios": "expo run:ios",
    "ios:build": "pnpm run firebase:ios && expo run:ios --no-bundler",
    "knip": "knip",
    "lingui": "pnpm lingui:extract && pnpm lingui:compile",
    "lingui:compile": "lingui compile --strict",
    "lingui:compile:watch": "lingui compile --watch --debounce 300",
    "lingui:extract": "lingui extract --clean",
    "lingui:extract:watch": "lingui extract --watch",
    "lingui:watch": "concurrently  \"pnpm lingui:extract:watch\"  \"pnpm lingui:compile:watch\"  ",
    "postinstall": "if [ \"$CI\" = \"1\" ]; then pnpm -w build:mobile; fi",
    "preandroid": "pnpm run firebase",
    "preios": "pnpm run check:cocoapods && pnpm run firebase",
    "start": "expo start",
    "storybook": "cross-env STORYBOOK_ENABLED='true' expo start",
    "storybook-generate": "sb-rn-get-stories -js",
    "storybook:android": "cross-env STORYBOOK_ENABLED='true' expo start --android",
    "storybook:ios": "cross-env STORYBOOK_ENABLED='true' expo start --ios",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "typecheck": "tsc --noEmit -p ./tsconfig.json"
  },
