{
  "cli": {
    "version": ">= 7.8.1",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "test": {
      "android": {
        "gradleCommand": ":app:assembleRelease :app:assembleAndroidTest -DtestBuildType=release",
        "withoutCredentials": true
      },
      "ios": {
        "simulator": true,
        "cocoapods": "1.15.2"
      },
      "channel": "test"
    },
    "simulator": {
      "node": "20.11.0",
      "pnpm": "9.6.0",
      "distribution": "internal",
      "developmentClient": true,
      "ios": {
        "simulator": true,
        "cocoapods": "1.15.2"
      },
      "channel": "simulator"
    },
    "development": {
      "node": "20.11.0",
      "pnpm": "9.6.0",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "cocoapods": "1.15.2"
      },
      "channel": "development"
    },
    "preview-simulator": {
      "node": "20.11.0",
      "pnpm": "9.6.0",
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "cocoapods": "1.15.2"
      },
      "channel": "preview-simulator"
    },
    "preview": {
      "node": "20.11.0",
      "pnpm": "9.6.0",
      "distribution": "internal",
      "ios": {
        "cocoapods": "1.15.2"
      },
      "channel": "preview"
    },
    "production": {
      "node": "20.11.0",
      "pnpm": "9.6.0",
      "distribution": "store",
      "autoIncrement": true,
      "ios": {
        "cocoapods": "1.15.2"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
