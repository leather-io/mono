{
  "cli": {
    "version": ">= 15.0.10",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "test": {
      "android": {
        "gradleCommand": ":app:assembleRelease :app:assembleAndroidTest -DtestBuildType=release",
        "withoutCredentials": true
      },
      "ios": {
        "simulator": true,
        "cocoapods": "1.16.2",
        "image": "macos-sequoia-15.3-xcode-16.2"
      }
    },
    "simulator-dev": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "cocoapods": "1.16.2",
        "image": "macos-sequoia-15.3-xcode-16.2"
      },
      "android": {
        "image": "latest"
      }
    },
    "simulator-pr": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "cocoapods": "1.16.2",
        "image": "macos-sequoia-15.3-xcode-16.2"
      },
      "android": {
        "image": "latest"
      }
    },
    "development": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2",
        "cocoapods": "1.16.2"
      }
    },
    "preview-simulator": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "distribution": "internal",
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2",
        "simulator": true,
        "cocoapods": "1.16.2"
      }
    },
    "preview": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "distribution": "internal",
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2",
        "cocoapods": "1.16.2"
      }
    },
    "production": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "distribution": "store",
      "autoIncrement": true,
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2",
        "cocoapods": "1.16.2"
      },
      "android": {
        "image": "latest"
      }
    },
    "maestro": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "environment": "preview",
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "image": "macos-sequoia-15.3-xcode-16.2",
        "cocoapods": "1.16.2"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"
      }
    }
  }
}
