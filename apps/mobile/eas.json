        "gradleCommand": ":app:assembleRelease :app:assembleAndroidTest -DtestBuildType=release",
        "withoutCredentials": true
      },
      "ios": {
        "simulator": true,
        "cocoapods": "1.15.2"
      }
    },
    "development": {
      "node": "20.11.0",
      "pnpm": "9.1.4",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "cocoapods": "1.15.2"
      }
    },
    "preview-simulator": {
      "node": "20.11.0",
      "pnpm": "9.1.4",
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "cocoapods": "1.15.2"
      }
    },
    "preview": {
      "node": "20.11.0",
      "pnpm": "9.1.4",
      "distribution": "internal",
      "ios": {
        "cocoapods": "1.15.2"
      }
    },
    "production": {
      "node": "20.11.0",
      "pnpm": "9.1.4",
      "distribution": "store",
      "autoIncrement": true,
      "ios": {
        "cocoapods": "1.15.2"
      }
    }
  },
  "submit": {
    "production": {}
  }
