{
  "cli": {
    "version": ">= 15.0.10",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "channel": "development",
      "distribution": "internal",
      "developmentClient": true,
      "ios": {
        "image": "latest",
        "cocoapods": "1.15.2"
      }
    },
    "staging": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "distribution": "internal",
      "channel": "staging",
      "autoIncrement": true,
      "ios": {
        "image": "latest",
        "cocoapods": "1.15.2"
      }
    },
    "production": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "channel": "production",
      "distribution": "store",
      "autoIncrement": true,
      "ios": {
        "image": "latest",
        "cocoapods": "1.15.2"
      },
      "android": {
        "image": "latest"
      }
    },
    "maestro": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "environment": "production",
      "withoutCredentials": true,
      "channel": "maestro",
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "image": "latest",
        "cocoapods": "1.15.2"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      }
    },
    "preview": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "simulator": true,
        "cocoapods": "1.15.2",
        "image": "latest"
      },
      "android": {
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"
      }
    }
  }
}
