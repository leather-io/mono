{
  "cli": {
    "version": ">= 15.0.10",
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "ios": {
        "image": "latest",
        "cocoapods": "1.15.2"
      },
      "android": {
        "image": "latest"
      }
    },
    "development": {
      "extends": "base",
      "channel": "development",
      "distribution": "internal",
      "developmentClient": true
    },
    "staging": {
      "extends": "base",
      "node": "20.11.0",
      "pnpm": "10.4.1",
      "distribution": "internal",
      "channel": "staging",
      "autoIncrement": true
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "distribution": "store",
      "autoIncrement": true
    },
    "maestro": {
      "extends": "base",
      "environment": "production",
      "withoutCredentials": true,
      "channel": "maestro",
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"
      }
    }
  }
}
