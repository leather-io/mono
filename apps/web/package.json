{
  "name": "@leather.io/web",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "NODE_OPTIONS='--import ./instrument.server.mjs' react-router build",
    "build:watch": "pnpm dev",
    "cf-typegen": "wrangler types",
    "deploy": "pnpm build && wrangler deploy",
    "dev": "NODE_OPTIONS='--import ./instrument.server.mjs' react-router dev",
    "fetch-posts": "node app/scripts/fetch-posts.js",
    "format": "prettier --write \"app/**/*.{ts,tsx}\" --ignore-path ../../.prettierignore",
    "format:check": "prettier --check \"app/**/*.{ts,tsx}\" --ignore-path ../../.prettierignore",
    "prebuild": "node ../../scripts/fetch-remote-posts.cjs",
    "predev": "node ../../scripts/fetch-remote-posts.cjs",
    "prepare": "pnpm cf-typegen && panda codegen",
    "preview": "npm run build && vite preview",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:unit": "vitest run",
    "typecheck": "pnpm cf-typegen && react-router typegen && tsc -b"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.1",
    "@leather.io/analytics": "workspace:*",
    "@leather.io/bitcoin": "workspace:*",
    "@leather.io/constants": "workspace:*",
    "@leather.io/models": "workspace:*",
    "@leather.io/panda-preset": "workspace:*",
    "@leather.io/query": "workspace:*",
    "@leather.io/rpc": "workspace:*",
    "@leather.io/sdk": "workspace:*",
    "@leather.io/services": "workspace:*",
    "@leather.io/stacks": "workspace:*",
    "@leather.io/ui": "workspace:*",
    "@leather.io/utils": "workspace:*",
    "@react-router/node": "7.6.2",
    "@react-router/serve": "7.6.2",
    "@segment/analytics-next": "1.81.0",
    "@sentry/profiling-node": "9.15.0",
    "@sentry/react-router": "9.15.0",
    "@stacks/network": "7.0.2",
    "@stacks/stacking": "7.0.5",
    "@stacks/stacks-blockchain-api-types": "7.14.1",
    "@stacks/transactions": "7.0.5",
    "@tanstack/react-query": "5.59.16",
    "@tanstack/react-table": "8.21.3",
    "axios": "1.8.4",
    "bignumber.js": "9.1.2",
    "content-security-policy-builder": "2.3.0",
    "dompurify": "3.2.4",
    "isbot": "5.1.27",
    "jotai": "2.12.4",
    "nprogress": "0.2.0",
    "radix-ui": "1.3.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.56.3",
    "react-router": "7.6.2",
    "safe-buffer": "5.2.1",
    "url-join": "5.0.0",
    "uuid": "11.1.0",
    "valid-url": "1.0.9",
    "vaul": "1.1.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.5.0",
    "@cloudflare/workers-types": "4.20250610.0",
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@pandacss/dev": "0.53.6",
    "@playwright/test": "1.52.0",
    "@react-router/cloudflare": "7.6.2",
    "@react-router/dev": "7.6.2",
    "@types/node": "22.15.3",
    "@types/nprogress": "0.2.3",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@types/valid-url": "1.0.7",
    "jsdom": "22.1.0",
    "react-router-devtools": "5.0.6",
    "rollup-plugin-node-polyfills": "0.2.1",
    "ts-node": "10.9.2",
    "tty-browserify": "0.0.1",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.9",
    "wrangler": "4.19.1"
  }
}
