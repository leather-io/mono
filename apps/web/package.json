    "@leather.io/rpc": "workspace:*",
    "@leather.io/sdk": "workspace:*",
    "@leather.io/ui": "workspace:*",
    "@leather.io/utils": "workspace:*",
    "@react-router/node": "7.4.0",
    "@react-router/serve": "7.4.0",
    "@stacks/network": "7.0.2",
    "@stacks/stacking": "7.0.5",
