{
  "name": "@leather.io/web",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "NODE_OPTIONS='--import ./instrument.server.mjs' react-router build",
    "cf-typegen": "wrangler types",
    "deploy": "pnpm build && wrangler deploy",
    "dev": "NODE_OPTIONS='--import ./instrument.server.mjs' react-router dev",
    "format": "prettier --write \"app/**/*.{ts,tsx}\" --ignore-path ../../.prettierignore",
    "format:check": "prettier --check \"app/**/*.{ts,tsx}\" --ignore-path ../../.prettierignore",
    "prepare": "pnpm cf-typegen && panda codegen",
    "preview": "npm run build && vite preview",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run",
    "typecheck": "pnpm cf-typegen && react-router typegen && tsc -b"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.1",
    "@leather.io/bitcoin": "workspace:*",
    "@leather.io/constants": "workspace:*",
    "@leather.io/models": "workspace:*",
    "@leather.io/panda-preset": "workspace:*",
    "@leather.io/query": "workspace:*",
    "@leather.io/rpc": "workspace:*",
    "@leather.io/sdk": "workspace:*",
    "@leather.io/stacks": "workspace:*",
    "@leather.io/ui": "workspace:*",
    "@leather.io/utils": "workspace:*",
    "@react-router/node": "7.5.2",
    "@react-router/serve": "7.5.2",
    "@sentry/profiling-node": "9.14.0",
    "@sentry/react-router": "9.14.0",
    "@stacks/network": "7.0.2",
    "@stacks/stacking": "7.0.5",
    "@stacks/stacks-blockchain-api-types": "7.14.1",
    "@stacks/transactions": "7.0.5",
    "@tanstack/react-query": "5.59.16",
    "@tanstack/react-table": "8.21.2",
    "axios": "1.8.4",
    "bignumber.js": "9.1.2",
    "isbot": "5.1.26",
    "jotai": "2.12.2",
    "nprogress": "0.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.56.1",
    "react-router": "7.5.2",
    "safe-buffer": "5.2.1",
    "url-join": "5.0.0",
    "valid-url": "1.0.9",
    "vaul": "1.1.2",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.0.11",
    "@cloudflare/workers-types": "4.20250424.0",
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@pandacss/dev": "0.53.4",
    "@playwright/test": "1.52.0",
    "@react-router/cloudflare": "7.5.2",
    "@react-router/dev": "7.5.2",
    "@types/node": "22.14.0",
    "@types/nprogress": "0.2.3",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@types/valid-url": "1.0.7",
    "react-router-devtools": "1.1.10",
    "rollup-plugin-node-polyfills": "0.2.1",
    "tty-browserify": "0.0.1",
    "typescript": "5.7.3",
    "vite": "6.2.5",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.9",
    "wrangler": "4.9.1"
  }
}
